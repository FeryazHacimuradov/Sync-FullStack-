@model Setting
@{
    ViewData["Title"] = "Index";
}

<div class="container">
    <div class="row">
        <div class="col-lg-12">
            <table class="table">
                <thead>
                    <tr>
                        <th scope="col">#</th>
                        <th scope="col">Logo</th>
                        <th scope="col">MainTitle</th>
                        <th scope="col">MainContent</th>
                        <th scope="col">Image</th>
                        <th scope="col">Email</th>
                        <th scope="col">Address</th>
                        <th scope="col">Phone</th>
                    </tr>
                </thead>
                <tbody>
                    <tr>
                        <th scope="row">@Model.Id</th>
                        <td>@Model.Logo</td>
                        <td>@Model.MainTitle</td>
                        <td>@Model.MainContent</td>
                        <td>@Model.Image</td>
                        <td>@Model.Email</td>
                        <td>@Model.Address</td>
                        <td>@Model.Phone</td>
                        <td>
                            <a asp-area="admin" asp-controller="setting" asp-action="update" asp-route-id="@Model.Id" class="btn btn-success">Update</a>
                        </td>
                    </tr>
                </tbody>
            </table>
        </div>
    </div>
</div>

